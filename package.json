{
  "name": "nextjs-express-mongoose-crudify-boilerplate",
  "version": "3.0.0",
  "description": "Next.js with Express boilerplate and Mongoose CRUD support",
  "main": "server/index.js",
  "scripts": {
    "d": "NODE_ENV=development nodemon -w server -w package.json server/index.js  --exec babel-node index.js --presets es2015 ",
    "build": "rm -rf build && babel server -d build/ --presets es2015 && next build && mv .next build/.next && cp package.json build",
    "start": "cd build && node index.js"
  },
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "connect-mongo": "^2.0.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "include-media": "^1.4.9",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "next": "^7.0.2",
    "next-compose-plugins": "^2.1.1",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.9",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.5.0",
    "prettier-eslint": "^8.8.2"
  }
}
